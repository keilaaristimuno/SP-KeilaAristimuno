            <Button fx:id="btnEliminar" layoutX="148.0" layoutY="310.0" mnemonicParsing="false" onAction="#eliminarProducto" text="Eliminar" />
            <Button fx:id="btnGuardar" layoutX="336.0" layoutY="335.0" mnemonicParsing="false" onAction="#guardarCSV" text="Guardar CSV" />
            <Button fx:id="btnInforme" layoutX="427.0" layoutY="335.0" mnemonicParsing="false" onAction="#generarInformeVencimientos" text="Generar Informe TXT" />

         </children>
      </Pane>
